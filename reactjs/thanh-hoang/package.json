{
  "name": "udt-core",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint \"*/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"*/**/*.{js,ts,tsx}\"",
    "eslint:ci": "eslint \"*/**/*.{js,ts,tsx}\" --quiet",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "preversion": "yarn config set version-tag-prefix \"release-v\"",
    "postversion": "git push --tags && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "postcss-normalize": "^10.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "typescript": "^5.3.2",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.0"
  },
  "packageManager": "yarn@1.22.22",

  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
